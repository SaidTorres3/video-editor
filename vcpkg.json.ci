{
  "name": "video-editor",
  "version": "0.1.0",
  "dependencies": [
    {
      "name": "ffmpeg",
      "features": [
        "dav1d",
        "openh264",
        "x264",
        "x265",
        "mp3lame",
        "fdk-aac",
        "opus",
        "zlib"
      ]
    },
    {
      "name": "curl",
      "features": [
        "sspi",
        "ssl",
        "schannel",
        "non-http"
      ]
    }
  ]
}
